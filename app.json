{
  "expo": {
    "version": "1.0.1",
    "name": "vidasana",
    "slug": "vidasana",
    "scheme": "vidasana",
    "orientation": "portrait",
    "icon": "./src/assets/icon.png",
    "platforms": ["ios", "android"],
    "assetBundlePatterns": ["**/*"],
    "userInterfaceStyle": "automatic",
    "experiments": {"typedRoutes": true, "tsconfigPaths": true},
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./src/assets/icon.png"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.vidasana",
      "infoPlist": {"ITSAppUsesNonExemptEncryption": false},
      "config": {"googleMapsApiKey": "AIzaSyD2Q1ArCXQ_-R_FBH8sXrRWwF6NDqHT2Hk"}
    },
    "android": {
      "package": "com.vidasana",
      "config": {"googleMaps": {"apiKey": "AIzaSyD2Q1ArCXQ_-R_FBH8sXrRWwF6NDqHT2Hk"}},
      "adaptiveIcon": {"backgroundColor": "#FFFFFF", "foregroundImage": "./src/assets/adaptive-icon.png"},
      "permissions": ["android.permission.ACCESS_COARSE_LOCATION", "android.permission.ACCESS_FINE_LOCATION"]
    },
    "plugins": [
      "expo-font",
      "expo-router",
      "expo-sqlite",
      "expo-secure-store",
      "expo-localization",
      ["expo-dev-client", {"launchMode": "most-recent"}],
      ["expo-splash-screen", {"imageWidth": 200, "backgroundColor": "#FFFFFF", "image": "./src/assets/logo.png"}],
      ["@stripe/stripe-react-native", {"enableGooglePay": true, "merchantIdentifier": "merchant.com.vidasana.app"}],
      ["expo-location", {"locationAlwaysAndWhenInUsePermission": "Allow $(PRODUCT_NAME) to use your location to search events near you."}]
    ],
    "extra": {"router": {}, "eas": {"projectId": "21bf8ff0-8d54-4bf4-9c0e-e8b94dff74db"}}
  }
}
